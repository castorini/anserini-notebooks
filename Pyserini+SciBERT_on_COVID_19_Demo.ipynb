      },
      "source": [
        "sim_matrices = []\n",
        "\n",
        "for pid in tqdm(range(len(doc_json['body_text']))):\n",
        "  sim_matrices.append(cross_match(query_state, paragraph_states[pid][-1]))"
      ],
